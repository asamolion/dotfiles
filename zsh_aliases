alias ..="cd .."
alias -- -="cd -"
alias cat="bat"
alias s.="subl ."

alias philu="subl $HOME/philu.sublime-project"
alias dedX="export OPENEDX_RELEASE=hawthorn.master && cd $HOME/arbisoft/edx/devstack && workon edx"
alias eucsd="cd $HOME/arbisoft/edx/edx-platform && workon edx"
alias sucsd="subl $HOME/ucsd.sublime-project"

alias edlystack="unset OPENEDX_RELEASE && cd $HOME/arbisoft/edly/edX/devstack && workon edx"

# project aliases
alias crfx="cd $HOME/workspace/fiverr/saloal/rfx"
alias srfx="subl $HOME/rfx.sublime-project"

alias sedly="subl $HOME/edly.sublime-project"


# Git aliaes
alias checkout="git checkout"
alias clone="git clone"
alias fetch="git fetch"
alias pull="git pull"
alias push="git push"
alias stat="git status"
alias branch="git branch"

alias cod4="docker start fervent_kowalevski"

alias pwd="pwd | xclip -selection c && pwd"
alias rmpyc="find . -name "*.pyc" -exec rm -f {} \;"

alias hostedit="sudoedit /etc/hosts"
alias xamppstart="sudo /opt/lampp/lampp start"
alias xamppstop="sudo /opt/lampp/lampp stop"

alias stopdocker="docker stop $(docker ps -a -q)"


