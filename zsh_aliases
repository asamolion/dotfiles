alias ..="cd .."
alias -- -="cd -"
alias cat="bat"
alias s.="subl ."

alias philu="subl $HOME/philu.sublime-project"
alias ducsd="export OPENEDX_RELEASE=hawthorn.master && workon devstack && cd $HOME/arbisoft/ucsd/edX/devstack"
alias sucsd="workon devstack && subl $HOME/ucsd.sublime-project"

alias dedly="unset OPENEDX_RELEASE && cd $HOME/arbisoft/edly/edX/devstack && workon edx"
alias sedly="unset OPENEDX_RELEASE && cd $HOME/arbisoft/edly/edX/devstack && workon edx"

# project aliases
alias crfx="cd $HOME/workspace/fiverr/saloal/rfx"
alias srfx="subl $HOME/rfx.sublime-project"

alias sedly="subl $HOME/edly.sublime-project"



alias pwd="pwd | xclip -selection c && pwd"

alias hostedit="sudoedit /etc/hosts"
alias xamppstart="sudo /opt/lampp/lampp start"
alias xamppstop="sudo /opt/lampp/lampp stop"

alias stopdocker="docker stop $(docker ps -a -q)"

alias mkvirtualenv3='mkvirtualenv --python=`which python3`'


